{
	"editor.fontSize": 13,
	"editor.fontFamily": "Verdana, Consolas, 'Courier New', monospace",
	"editor.lineHeight": 24,
	"editor.letterSpacing": -0.15,
	"editor.renderWhitespace": "all",
	"editor.detectIndentation": false,
	"editor.insertSpaces": false,
	"editor.tabSize": 4,
	"editor.roundedSelection": false,
	"editor.cursorBlinking": "solid",
	"editor.formatOnPaste": true,
	"editor.minimap.maxColumn": 30,
	"editor.minimap.renderCharacters": false,
	"editor.minimap.showSlider": "always",
	"editor.minimap.side": "right",
	"editor.trimAutoWhitespace": true,
	"editor.useTabStops": true,
	"git.enableSmartCommit": true,
	"git.confirmSync": false,
	"git.confirmEmptyCommits": false,
	"merge-conflict.autoNavigateNextConflict.enabled": true,
	"explorer.openEditors.visible": 20,
	"explorer.sortOrder": "filesFirst",
	"keyboard.dispatch": "keyCode",
	"window.titleBarStyle": "custom",
	"window.zoomLevel": 0,
	"workbench.startupEditor": "none",
	"workbench.colorTheme": "Default Light+",
	"workbench.enableExperiments": false,
	"workbench.enableLegacyStorage": false,
	"workbench.statusBar.feedback.visible": false,
	"workbench.editor.closeOnFileDelete": true,
	"workbench.tips.enabled": false,
	"workbench.iconTheme": "vs-minimal",
	"workbench.quickOpen.closeOnFocusLost": false,
	"editor.codeActionsOnSaveTimeout": 500,
	"http.proxyStrictSSL": false,
	"update.channel": "none",
	"update.enableWindowsBackgroundUpdates": false,
	"update.showReleaseNotes": false,
	"telemetry.enableCrashReporter": false,
	"telemetry.enableTelemetry": false,
	"files.autoSave": "onFocusChange",
	"files.encoding": "utf8",
	"files.autoSaveDelay": 500,
	"files.maxMemoryForLargeFilesMB": 1024,
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/node_modules": true
	},
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/**": true,
		"**/*.log": true,
		"**/**.log": true,
		"**/**._js": true,
		"**/build/**": true,
		"**/src/_/**": true
	},
	"search.showLineNumbers": true,
	"search.useGlobalIgnoreFiles": true,
	"search.useIgnoreFiles": true,
	"search.exclude": {
		"**/.git": true,
		"**/node_modules": true,
		"**/build": true,
		"**/cache": true,
		"**/src/_": true
	}
}