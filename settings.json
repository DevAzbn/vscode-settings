{
	"window.titleBarStyle": "custom",
	"window.zoomLevel": 0,
	"http.proxyStrictSSL": false,
	"update.channel": "none",
	"update.enableWindowsBackgroundUpdates": false,
	"update.showReleaseNotes": false,
	"telemetry.enableCrashReporter": false,
	"telemetry.enableTelemetry": false,
	"editor.fontSize": 13,
	"editor.fontFamily": "Verdana, Consolas, 'Courier New', monospace",
	"editor.lineHeight": 24,
	"editor.letterSpacing": -0.15,
	"editor.renderWhitespace": "all",
	"editor.detectIndentation": false,
	"editor.insertSpaces": false,
	"editor.tabSize": 4,
	"editor.roundedSelection": false,
	"editor.cursorBlinking": "solid",
	"editor.formatOnPaste": false,
	"editor.minimap.maxColumn": 30,
	"editor.minimap.renderCharacters": false,
	"editor.minimap.showSlider": "always",
	"editor.minimap.side": "right",
	"editor.trimAutoWhitespace": true,
	"editor.useTabStops": true,
	"editor.codeActionsOnSaveTimeout": 500,
	"workbench.startupEditor": "none",
	"workbench.enableExperiments": false,
	"workbench.enableLegacyStorage": false,
	"workbench.statusBar.feedback.visible": false,
	"workbench.editor.closeOnFileDelete": true,
	"workbench.tips.enabled": false,
	"workbench.iconTheme": "vs-minimal",
	"workbench.quickOpen.closeOnFocusLost": false,
	"explorer.openEditors.visible": 20,
	"explorer.sortOrder": "filesFirst",
	"git.enableSmartCommit": true,
	"git.confirmSync": false,
	"git.confirmEmptyCommits": false,
	"merge-conflict.autoNavigateNextConflict.enabled": true,
	"keyboard.dispatch": "keyCode",
	"files.autoSave": "onFocusChange",
	"files.encoding": "utf8",
	"files.autoSaveDelay": 500,
	"files.maxMemoryForLargeFilesMB": 1024,
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/node_modules": true,
		"**/package-lock.json": true
	},
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/**": true,
		"**/*.log": true,
		"**/**.log": true,
		"**/**._js": true,
		"**/build/**": true,
		"**/src/_/**": true,
		"**/package-lock.json": true
	},
	"search.showLineNumbers": true,
	"search.useGlobalIgnoreFiles": true,
	"search.useIgnoreFiles": true,
	"search.exclude": {
		"**/.git": true,
		"**/node_modules": true,
		"**/build": true,
		"**/cache": true,
		"**/src/_": true,
		"**/package-lock.json": true
	},
	"javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
	"javascript.format.placeOpenBraceOnNewLineForFunctions": false,
	"javascript.format.placeOpenBraceOnNewLineForControlBlocks": false,
	"terminal.integrated.shell.linux": "bash",
	"indentRainbow.updateDelay": 500,
	"indentRainbow.errorColor": "rgba(255,0,0,0.25)",
	"indentRainbow.colors": [
		"rgba(0,0,0,0.15)",
		"rgba(32,32,32,0.15)",
		"rgba(64,64,64,0.15)",
		"rgba(96,96,96,0.15)",
		"rgba(128,128,128,0.15)",
		"rgba(160,160,160,0.15)",
		"rgba(192,192,192,0.15)",
		"rgba(224,224,224,0.15)"
	],
	"terminal.integrated.rendererType": "dom"
}